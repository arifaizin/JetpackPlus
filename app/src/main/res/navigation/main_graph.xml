<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/mainTabFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/mainTabFragment"
        android:name="com.arifaizin.jetpackpro.ui.fragment.MainTabFragment"
        android:label="fragment_main_tab"
        tools:layout="@layout/fragment_main_tab">
        <action
            android:id="@+id/action_mainTabFragment_to_favoriteNav"
            app:destination="@id/favoriteNav" />
    </fragment>

    <include-dynamic
        android:id="@+id/favoriteNav"
        app:moduleName="favorite"
        app:graphResName="favorite_graph"/>

</navigation>